
        <%- include("./partials/Head")  %> 

        <img class="wave" src="https://raw.githubusercontent.com/sefyudem/Responsive-Login-Form/master/img/wave.png">
	<div class="container">
		<div class="img">
			<img src="https://raw.githubusercontent.com/sefyudem/Responsive-Login-Form/master/img/bg.svg">
		</div>
		<div class="login-content">
			<form action="/api/users" method="post">
				<img src="/images/profile _icon.png">
				<h2 class="title">Welcome</h2>
                <%- include("./partials/messages") %>
           		<div class="input-div one">
           		   <div class="div">
           		   		<input type="text" name="first_name" placeholder="First Name" class="input" value="<% typeof first_name !='undefined' ? first_name : '' %>" >
           		   </div>
               	</div>
                   <div class="input-div one">
                    <div class="div">
                            <input type="text" name="second_name" placeholder="Second Name" class="input" value="<% typeof second_name !='undefined' ? second_name : '' %>" >
                    </div>
                 </div>
                 <div class="input-div one">
                    <div class="div">
                            <input name="email" type="email" placeholder="Email Address" class="input" value="<% typeof email !='undefined' ? email : '' %>" >
                    </div>
                 </div>
                 <div class="input-div one">
                    <div class="div">
                            <input type="password" name="password" placeholder="Password" class="input" value="<% typeof password !='undefined' ? password : '' %>">
                    </div>
                 </div>
                 
                 <div class="input-div one">
                        <div class="div">
                                <input type="password"name="password2" placeholder="Confirm Password" class="input" value="<% typeof password2 !='undefined' ? password2 : '' %>">
                        </div>
                </div>
                 <div class="input-div one">
                    <div class="div">
                            <input type="number" name="phonenumber" placeholder="Phone Number" class="input" value="<% typeof phonenumber !='undefined' ? phonenumber : '' %>">
                    </div>
                 </div>
                 <div class="row">
                        <label for="rol" class="form-label col-2">Plan :</label>
                        <select name="plan" class="form-select border col py-2" id="state" value="<%= typeof name != 'undefined' ? name : '' %>" >
                                <option value="basic">Basic</option>
                                <option value="premium" >Premium</option>
                        </select>
                 </div>
            	<a href="#" class="my-3 a">Forgot Password?</a>
            	<input type="submit" class="btn" value="Sign UP">
                <p class="mt-3">You already have an account ? <a href="/signin">Sign in</a></p>

            </form>
        </div>
    </div>
    
            <%- include("./partials/footer") %> 
